{
    "manifest_version": 2,
    "name": "Wayback Finder",
    "description": "A Google Chrome extension that allows you to easily find and store web pages on the Wayback Machine.",
    "version": "1.0.0",
    "icons": {"128": "images/icon128.png"},
    "browser_action": {
        "default_icon": "images/icon128.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://*/",
        "http://*/"
    ]
}